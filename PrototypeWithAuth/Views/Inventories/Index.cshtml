@model IEnumerable<PrototypeWithAuth.Models.Inventory>

@{
    ViewData["Title"] = "Index";
    Layout = "View";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ItemReceivedDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ItemPaymentDate)
            </th>
            <!--<th>
        @Html.DisplayNameFor(model => model.InventorySubcategory)
    </th>-->
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ItemReceivedDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ItemPaymentDate)
            </td>
          <!--  <td>
                @Html.DisplayFor(modelItem => item.InventorySubcategory.InventorySubcategoryDescription)
            </td> -->
            <td>
                <a asp-action="Edit" asp-route-id="@item.InventoryID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.InventoryID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.InventoryID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
