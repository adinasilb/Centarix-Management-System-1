@model PrototypeWithAuth.ViewModels.RequestListIndexViewModel
@using PrototypeWithAuth.AppData;
@using PrototypeWithAuth.Data;
@using Microsoft.AspNetCore.Identity
@inject SignInManager<ApplicationUser> SignInManager

@{
    ViewData["Title"] = "My Lists";

    Layout = "~/Views/Shared/RequestNavView.cshtml";
    if (AppUtility.IsAjaxRequest(this.Context.Request))
    {
        Layout = null;
    }

}
@if (Model.ErrorMessage != null)
{
    <div class="row p-4">
        <span class="text danger-text view-bag-error">Error: @Model.ErrorMessage</span>
    </div>
}
<div class="_IndexTableListTabs">
    @{ await Html.RenderPartialAsync("_IndexTableWithListTabs", Model);}
</div>
<script src="~/js/PricePopoverScript.js"></script>
<script src="~/js/RequestLists.js"></script>
