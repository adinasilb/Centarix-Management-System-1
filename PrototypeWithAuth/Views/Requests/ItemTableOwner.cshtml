@model PrototypeWithAuth.ViewModels.RequestIndexPartialViewModel
@using PrototypeWithAuth.AppData
@using React.AspNet
@{
    ViewData["Title"] = "Owner";

    Layout = "~/Views/Shared/RequestNavView.cshtml";
}
<div class="row pb-3">
    <a style="z-index:100" href="@Url.ActionLink("Index", "ApplicationUsers", new { @PageType = TempData[AppUtility.TempDataTypes.PageType.ToString()], @SectionType = Model.InventoryFilterViewModel.SectionType.ToString() }).ToString()"
       class="title-label  btn-link heading-1 ">
        <i class="icon-arrow_back_ios-24px-1" style="font-size:1.4rem;"></i>
        <label class="pl-2">@Model.SidebarFilterName</label>
    </a>
</div>

@Html.React("RootComponent", new { viewModel = Model, viewEnum = "/_IndexTable" })

@Html.ReactGetScriptPaths()
@Html.ReactInitJavaScript()


