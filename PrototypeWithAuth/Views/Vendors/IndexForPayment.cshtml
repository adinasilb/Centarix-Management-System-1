@model IEnumerable<PrototypeWithAuth.Models.Vendor>
@using PrototypeWithAuth.AppData
@{
    ViewData["Title"] = "Suppliers";

    Layout = "~/Views/Shared/RequestNavView.cshtml";
}

<input type="hidden" value="@TempData[AppUtility.TempDataTypes.MenuType.ToString()]" id="SectionType" />
<div class="container-fluid">
    <div class="row">
        <div class="col-2 offset-7">
            <input type="text" placeholder="Search By Name" class="mb-2 w-100 form-control-plaintext  border-bottom search-by-name" />
        </div>
        <div class="col-3">
            <input type="text" placeholder="Search By Company ID" class="mb-2 w-100 form-control-plaintext border-bottom search-by-company-ID" />
        </div>
    </div>
</div>
<div class="indexTable _IndexForPayment scroll-page ">
    @{ await Html.RenderPartialAsync("_IndexForPayment", Model);}
</div>
<script src="~/js/vendorFilter.js"></script>