    $.fn.CallOrderLateModal = function (id) {
        console.log("in call modal, id: " + id);
        $('.modal').replaceWith('');
        $(".modal-backdrop").remove();
        $('#loading').show();
        //var url = @Url.Action("Requests", "OrderLate");
        //var url = "Requests/OrderLateModal/?id=" + id;
        $.ajax({
            async: true,
            type: 'GET',
            cache: false,
            url: '@(Url.Action("OrderLateModal", "Requests"))',
            data: id,
            dataType: "string",
            success: function (data) {
                $('#loading').hide();
                var modal = $(data);
                $('body').append(modal);
                $(".modal-view").modal({
                    backdrop: true,
                    keyboard: false,
                });
                $(".modal").modal('show');
            }
        });
        return false;
    };

