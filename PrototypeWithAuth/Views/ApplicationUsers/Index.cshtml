@model IEnumerable<PrototypeWithAuth.Data.ApplicationUser>
@using PrototypeWithAuth.AppData
@{
    ViewData["Title"] = "Index";
    var pagetype = TempData["PageType"];
    Layout = "~/Views/Shared/RequestNavView.cshtml";
}

<h1>Index</h1>
<table class="table">
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>

                    @Html.ActionLink(item.FirstName, "Index", "Requests", new { @applicationUserID =  @item.Id, @PageType = pagetype }, null)
                </td>
            </tr>
        }
    </tbody>
</table>


