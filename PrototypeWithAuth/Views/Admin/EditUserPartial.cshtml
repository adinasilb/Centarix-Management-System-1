@model PrototypeWithAuth.ViewModels.RegisterUserViewModel;
@using PrototypeWithAuth.AppData;
@{

    string showDetails = "";
    string showSalary = "";
    string showSecurity = "";
    string showPermissions = "";
    string showBudget = "";
    string showImage = "";
    switch (Model.Tab)
    {
        case 1:
            showDetails = " active show ";
            break;
        case 2:
            if (Model.NewEmployee.EmployeeStatusID == 1)
            {
                showSalary = " active show ";
            }
            break;
        case 3:
            showSecurity = " active show ";
            break;
        case 4:
            showPermissions = " active show ";
            break;
        case 5:
            showBudget = " active show ";
            break;
        case 6:
            showImage = " active show ";
            break;

    }

    string hidden = "";
    if (Model.NewEmployee.EmployeeStatusID == 4)
    {
        hidden = "hidden";
    }
}

<script src="~/js/confirmedit.js"></script>
<script src="~/js/site.js" asp-append-version="true"></script>
<script src="~/js/Validation/UsersFormValidation.js"></script>
<script src="~/js/validate.js" asp-append-version="true"></script>
<script src="~/js/CreateUser.js"></script>

<input class="hasErrorMessage" type="hidden" value="@ViewBag.ErrorMessage" />
<input type="hidden" asp-for="NewEmployee.IsUser" />
<div class="container-fluid edit-modal-body box-shadow" style="background: #FFF; padding: 2em; border: 2px solid #EAEAEB;">
    <div class=" row text-right mb-6">
        <div class="switch col-12 switch-margin">
            <label>
                <label class="edit-mode-switch-description">Edit Mode Off</label>
                <input type="checkbox" class="turn-edit-on-off users" value="@Model.ApplicationUserID" name="details">
                <span class="lever"></span>
            </label>
        </div>
    </div>
    <div class="tab-content">
        <div id="details" class="tab-pane fade fade in @showDetails">
            <div class="row border-bottom pb-5 px-5 text-center " style="
    margin-right: -4rem;
    margin-left: -4rem;
">
                <input type="hidden" value="@Model.NewEmployee.EmployeeStatusID" id="OriginalStatusID" centarixID="@Model.CentarixID" />
                <input type="hidden" asp-for="NewEmployee.EmployeeStatusID" />
                @{ var first = true; }
                @foreach (var status in Model.EmployeeStatuses)
                {
                    var id = "EmployeeStatusID" + status.EmployeeStatusID;
                    if (first)
                    {
                        <div class="custom-control custom-radio mr-4  employee-type">
                            @Html.RadioButtonFor(model => model.NewEmployee.EmployeeStatusID, status.EmployeeStatusID, htmlAttributes: new { @class = "custom-control-input mark-readonly employee-status-radio", @id = id, @checked = "checked", @disabled = "disabled" })
                            <label class="radio-button custom-control-label pt-1 pl-2" for="@id">@status.Description</label>
                        </div>
                        first = false;
                    }
                    else
                    {
                        <div class="custom-control custom-radio mr-4 ml-4 employee-type">
                            @Html.RadioButtonFor(model => model.NewEmployee.EmployeeStatusID, status.EmployeeStatusID, htmlAttributes: new { @class = "custom-control-input mark-readonly employee-status-radio", @id = id, @disabled = "disabled" })
                            <label class="radio-button custom-control-label pt-1 pl-2" for="@id">@status.Description</label>
                        </div>
                    }
                }
                <br /><span id="validation-EmployeeStatus" class="hidden text-danger-centarix">Please choose a valid Employee Status</span>
            </div>
            <div class="row">
                <div class="col-6 p-0">
                    <label asp-for="FirstName" class="control-label"></label>
                    <input asp-for="FirstName" class="form-control-plaintext border-bottom mark-readonly" disabled />
                    <span asp-validation-for="FirstName" class="text-danger-centarix"></span>
                </div>
                <div class="col-6">
                    <label asp-for="LastName" class="control-label"></label>
                    <input asp-for="LastName" class="form-control-plaintext border-bottom mark-readonly" disabled />
                    <span asp-validation-for="LastName" class="text-danger-centarix"></span>
                </div>
            </div>

            <div class="row">
                <div class="col-6  p-0">
                    <label asp-for="CentarixID" class="control-label"></label>
                    <input asp-for="CentarixID" class="form-control-plaintext border-bottom " disabled />
                    <span asp-validation-for="CentarixID" class="text-danger-centarix"></span>
                </div>
                <div class="col-6 ">
                    <label asp-for="Email" class="control-label "></label>
                    <input asp-for="Email" class="form-control-plaintext border-bottom mark-readonly" autocomplete="nope" type="text" disabled />
                    <span asp-validation-for="Email" class="text-danger-centarix"></span>
                </div>
            </div>
            <div class="row only-employee @hidden">
                <div class="col-6  p-0 only-employee @hidden">
                    <label asp-for="NewEmployee.DOB" class="control-label only-employee"></label>
                    <input asp-for="NewEmployee.DOB" class="form-control-plaintext border-bottom only-employee mark-readonly" disabled />
                    <span asp-validation-for="NewEmployee.DOB" class="text-danger-centarix"></span>
                </div>
