@model PrototypeWithAuth.ViewModels.RegisterUserViewModel;
@using PrototypeWithAuth.AppData;
@{

    string showDetails = "";
    string showSalary = "";
    string showSecurity = "";
    string showPermissions = "";
    string showBudget = "";
    string showImage = "";
    switch (Model.Tab)
    {
        case 1:
            showDetails = " active show ";
            break;
        case 2:
            if (Model.Employee.EmployeeStatusID == 1)
            {
                showSalary = " active show ";
            }
            break;
        case 3:
            showSecurity = " active show ";
            break;
        case 4:
            showPermissions = " active show ";
            break;
        case 5:
            showBudget = " active show ";
            break;
        case 6:
            showImage = " active show ";
            break;

    }
    var isedittable = "disabled";

    var modaltype = "edit";
    var blankage = "";
    if (Model.SidebarEnum == AppUtility.SidebarEnum.Add)
    {
        modaltype = "add";
        isedittable = "";
        blankage = "value=''";
    }

    string hidden = "";
    if (Model.Employee.EmployeeStatusID == 4)
    {
        hidden = "hidden";
    }
    var secureapppassBool = Model.Employee.SecureAppPass == null ? false : true;
}

<script src="~/js/Validation/UsersFormValidation.js"></script>
<script src="~/js/validate.js"></script>
<script src="~/js/confirmedit.js"></script>
<script src="~/js/site.js"></script>
<script src="~/js/CreateUser.js"></script>


<input type="hidden" asp-for="Employee.IsUser" />
<input type="hidden" id="hiddenSecureAppPass" value="@secureapppassBool" />
<div class="container-fluid edit-modal-body box-shadow" style="background: #FFF; padding: 2em; border: 2px solid #EAEAEB;">
    <div class="row">
        @if (Model.ErrorMessage != null)
        {
            <span class="text view-bag-error error-message">Error: @Model.ErrorMessage</span>
        }
    </div>
    @if (modaltype == "edit")
    {
        <div class=" row text-right mb-6">
            <div class="switch col-12 switch-margin">
                <label>
                    <label class="edit-mode-switch-description">Edit Mode Off</label>
                    <input type="checkbox" class="turn-edit-on-off users" value="@Model.Employee.Id" name="details" currentEmail="@Model.Employee.Email">
                    <span class="lever"></span>
                </label>
            </div>
        </div>
    }
    <div class="tab-content">
        <div id="details" class="tab-pane fade fade in @showDetails">
            <div class="row border-bottom pb-5 px-5 text-center " style="
    margin-right: -4rem;
    margin-left: -4rem;
">
                <input type="hidden" value="@Model.Employee.EmployeeStatusID" id="OriginalStatusID" centarixID="@Model.CentarixID" />
                <input type="hidden" asp-for="Employee.EmployeeStatusID" />
                @{ var first = true; }
                @foreach (var status in Model.EmployeeStatuses)
                {
                    var id = "EmployeeStatusID" + status.EmployeeStatusID;
                    if (first)
                    {
                        <div class="custom-control custom-radio mr-4  employee-type">
                            @Html.RadioButtonFor(model => model.Employee.EmployeeStatusID, status.EmployeeStatusID, htmlAttributes: new { @class = "custom-control-input mark-readonly employee-status-radio", @id = id, @checked = "checked", @isedittable })
                            <label class="radio-button custom-control-label pt-1 pl-2" for="@id">@status.Description</label>
                        </div>
                        first = false;
                    }
                    else
                    {
                        <div class="custom-control custom-radio mr-4 ml-4 employee-type">
                            @Html.RadioButtonFor(model => model.Employee.EmployeeStatusID, status.EmployeeStatusID, htmlAttributes: new { @class = "custom-control-input mark-readonly employee-status-radio", @id = id, @isedittable })
                            <label class="radio-button custom-control-label pt-1 pl-2" for="@id">@status.Description</label>
                        </div>
                    }
                }
                <br /><span id="validation-EmployeeStatus" class="hidden text-danger-centarix">Please choose a valid Employee Status</span>
            </div>
            <div class="row">
                <div class="col-6 p-0">
                    <label asp-for="Employee.FirstName" class="control-label"></label>
                    <input asp-for="Employee.FirstName" class="form-control-plaintext border-bottom mark-readonly @isedittable" />
                    <span asp-validation-for="Employee.FirstName" class="text-danger-centarix"></span>
                </div>
                <div class="col-6">
                    <label asp-for="Employee.LastName" class="control-label"></label>
                    <input asp-for="Employee.LastName" class="form-control-plaintext border-bottom mark-readonly @isedittable" />
                    <span asp-validation-for="Employee.LastName" class="text-danger-centarix"></span>
                </div>
            </div>

            <div class="row">
                <div class="col-3  p-0">
                    <label asp-for="CentarixID" class="control-label"></label>
                    <input asp-for="CentarixID" class="form-control-plaintext border-bottom " readonly />
                    <span asp-validation-for="CentarixID" class="text-danger-centarix"></span>
                </div>
                <div class="col-3 only-employee @hidden">
                    <label asp-for="Employee.IDNumber" class="control-label only-employee"></label>
                    <input asp-for="Employee.IDNumber" type="text" class="form-control-plaintext border-bottom mark-readonly only-employee @isedittable" />
                    <span asp-validation-for="Employee.IDNumber" class="text-danger-centarix"></span>
                </div>
                <div class="col-6 ">
                    <label asp-for="Employee.Email" class="control-label "></label>
                    <input asp-for="Employee.Email" class="form-control-plaintext border-bottom mark-readonly @isedittable" autocomplete="nope" type="text" />
                    <span asp-validation-for="Employee.Email" class="text-danger-centarix"></span>
                </div>
            </div>
            <div class="row only-employee @hidden">
                <div class="col-6  p-0 only-employee @hidden">
                    <label asp-for="Employee.DOB" class="control-label only-employee"></label>
                    @if (modaltype == "add")
                    {
                        <input asp-for="Employee.DOB" type="text" value="" class="form-control-plaintext border-bottom only-employee mark-readonly datepicker @isedittable" asp-format="@AppUtility.AspDateFormatString" />
                    }
                    else
                    {
                        <input asp-for="Employee.DOB" type="text" class="form-control-plaintext border-bottom only-employee mark-readonly datepicker @isedittable" asp-format="@AppUtility.AspDateFormatString" />
                    }
                    <span asp-validation-for="Employee.DOB" class="text-danger-centarix"></span>
                </div>
                <div class="col-6 only-employee @hidden">
                    <label asp-for="Employee.Age" class="control-label"></label>
                    <input asp-for="Employee.Age" name="Age" class="form-control-plaintext border-bottom @isedittable " />
                </div>
            </div>
            <div class="row">
                <div class="col-4  p-0 only-employee @hidden">
                    <label class="control-label only-employee">Job Category</label>
                    @Html.DropDownListFor(
                        m => m.Employee.JobSubcategoryType.JobCategoryTypeID,
                        new SelectList(
                        Model.JobCategoryTypes,
                        dataValueField: "JobCategoryTypeID",
                        dataTextField: "Description"),
                       "Select A Category",
                    new { @class = "mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly job-category", @searchable = "default value", @isedittable })
                    @*<select class="mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly job-category" asp-for="NewEmployee.JobSubcategoryType.JobCategoryTypeID" searchable="default value" disabled>
                            @foreach (var category in Model.JobCategoryTypes)
                            {
                                <option value="@category.JobCategoryTypeID">@category.Description</option>
                            }
                        </select>*@
                    <span asp-validation-for="Employee.JobSubcategoryType.JobCategoryTypeID" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 only-employee @hidden">
                    <label class="control-label only-employee @hidden">Job Subcategory</label>
                    @Html.DropDownListFor(
                    m => m.Employee.JobSubcategoryTypeID,
                    new SelectList(
                        Model.JobSubcategoryTypes,
                        dataValueField: "JobSubcategoryTypeID",
                        dataTextField: "Description"),
                        optionLabel: "Select A Subcategory",
                    new { @class = "mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly job-subcategory remove-extra-border", @searchable = "default value", @isedittable, @id = "job-subcategory" })
                    @*<select class="mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly job-category" asp-for="NewEmployee.JobSubcategoryTypeID" searchable="default value" disabled>
                            @foreach (var category in Model.JobSubcategoryTypes)
                            {
                                <option value="@category.JobSubcategoryTypeID">@category.Description</option>
                            }
                        </select>*@
                    <span asp-validation-for="Employee.JobSubcategoryTypeID" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 remove-padding-user @(hidden=="hidden"?"p-0":"")">
                    <label asp-for="Employee.PhoneNumber" class="control-label"></label>
                    <input asp-for="Employee.PhoneNumber" class="form-control-plaintext border-bottom mark-readonly @isedittable" />
                    <span asp-validation-for="Employee.PhoneNumber" class="text-danger-centarix"></span>
                </div>
            </div>
            <div class="row only-employee @hidden">
                <div class="col-4  p-0 only-employee @hidden">
                    <label asp-for="Employee.PhoneNumber2" class="control-label"></label>
                    <input asp-for="Employee.PhoneNumber2" class="form-control-plaintext border-bottom mark-readonly @isedittable" />
                    <span asp-validation-for="Employee.PhoneNumber2" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 only-employee">
                    <label class="control-label only-employee">Degree</label>
                    @Html.DropDownListFor(
                        vm => vm.Employee.DegreeID,
                        new SelectList(
                        Model.Degrees,
                        dataValueField: "DegreeID",
                        dataTextField: "Description"),
                        optionLabel: "Select A Degree",
                        htmlAttributes: new { @class = "mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly @isedittable", @searchable = "default value" }
                        )
                    <span asp-validation-for="Employee.DegreeID" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 only-employee @hidden">
                    <label asp-for="Employee.StartedWorking" class="control-label only-employee"></label>
                    @if (modaltype == "add")
                    {
                        <input asp-for="Employee.StartedWorking" value="" type="text" class="form-control-plaintext border-bottom only-employee mark-readonly datepicker @isedittable" asp-format="@AppUtility.AspDateFormatString" />
                    }
                    else
                    {
                        <input asp-for="Employee.StartedWorking" type="text" class="form-control-plaintext border-bottom only-employee mark-readonly datepicker @isedittable" asp-format="@AppUtility.AspDateFormatString" />
                    }
                    <span asp-validation-for="Employee.StartedWorking" class="text-danger-centarix"></span>
                </div>
            </div>
            <div class="row only-employee @hidden">
                <div class="col-4  p-0 only-employee @hidden">
                    <label class="control-label only-employee">Relationship Status</label>
                    @Html.DropDownListFor(
                        vm => vm.Employee.MaritalStatusID,
                        new SelectList(
                        Model.MaritalStatuses,
                        dataValueField: "MaritalStatusID",
                        dataTextField: "Description"),
                        optionLabel: "Select A Status",
                        htmlAttributes: new { @class = "mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly @isedittable", @searchable = "default value" }
                        )
                    <span asp-validation-for="Employee.MaritalStatusID" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 only-employee @hidden">
                    <label class="control-label only-employee">Citizenship</label>
                    @Html.DropDownListFor(
                        vm => vm.Employee.CitizenshipID,
                        new SelectList(
                        Model.Citizenships,
                        dataValueField: "CitizenshipID",
                        dataTextField: "Description"),
                        optionLabel: "Select A Country",
                        htmlAttributes: new { @class = "mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly @isedittable", @searchable = "default value" }
                        )
                    <span asp-validation-for="Employee.DegreeID" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 only-employee @hidden">
                    <label asp-for="Employee.TaxCredits" class="control-label only-employee"></label>
                    <input asp-for="Employee.TaxCredits" class="form-control-plaintext border-bottom only-employee mark-readonly @isedittable" />
                    <span asp-validation-for="Employee.TaxCredits" class="text-danger-centarix"></span>
                </div>
            </div>
        </div>
        <div id="salary" class="tab-pane fade @showSalary">
            <div class="row">
                <div class="col-4  p-0">
                    <label class="control-label" asp-for="EmployeeWorkScope"></label>
                    <div class="input-group">
                        <input class="form-control-plaintext border-bottom mark-readonly @isedittable" asp-for="EmployeeWorkScope" />
                        <div class="input-group-text">%</div>
                    </div>
                    <span asp-validation-for="EmployeeWorkScope" class="text-danger-centarix"></span>
                </div>

                <div class="col-4">
                    <label class="control-label" asp-for="Employee.SalariedEmployee.HoursPerDay"></label>
                    <input type="hidden" asp-for="Employee.SalariedEmployee.HoursPerDay" />
                    @{
                        double hoursWorked = Model.Employee.SalariedEmployee?.HoursPerDay ?? 0;
                    }
                    <input type="text" id="TimeSpan-HoursPerDay" name="TimeSpan-HoursPerDay" class="form-control-plaintext border-bottom mark-readonly @isedittable" value="@hoursWorked" />
                    <span asp-validation-for="Employee.SalariedEmployee.HoursPerDay" class="text-danger-centarix"></span>
                </div>
                <div class="col-4">
                    <label class="control-label" asp-for="Employee.VacationDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly @isedittable" asp-for="Employee.VacationDays" />
                    <span class="text-danger-centarix" asp-validation-for="Employee.VacationDays"></span>
                </div>
            </div>
            <div class="row">
                <div class="col-4 pl-0">
                    <label class="control-label" asp-for="Employee.RollOverVacationDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly  @isedittable" asp-for="Employee.RollOverVacationDays" />
                    <span class="text-danger-centarix" asp-validation-for="Employee.RollOverVacationDays"></span>
                </div>
                <div class="col-4">
                    <label class="control-label" asp-for="Employee.RollOverSickDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly  @isedittable" asp-for="Employee.RollOverSickDays" />
                    <span class="text-danger-centarix" asp-validation-for="Employee.RollOverSickDays"></span>
                </div>
                <div class="col-4 pl-0">
                    <label class="control-label" asp-for="Employee.SpecialDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly  @isedittable" asp-for="Employee.SpecialDays" />
                    <span class="text-danger-centarix" asp-validation-for="Employee.SpecialDays"></span>
                </div>
            </div>
            @*<div class="row">
                    <div class="col-4 pl-0">
                        <label class="control-label" asp-for="NewEmployee.BonusVacationDays"></label>
                        <input class="form-control-plaintext border-bottom mark-readonly" asp-for="NewEmployee.BonusVacationDays" disabled />
                        <span class="text-danger-centarix" asp-validation-for="NewEmployee.BonusVacationDays"></span>
                    </div>
                    <div class="col-4">
                        <label class="control-label" asp-for="NewEmployee.BonusSickDays"></label>
                        <input class="form-control-plaintext border-bottom mark-readonly" asp-for="NewEmployee.BonusSickDays" disabled />
                        <span class="text-danger-centarix" asp-validation-for="NewEmployee.BonusSickDays"></span>
                    </div>
                </div>*@
        </div>

        <div id="security" class="tab-pane fade @showSecurity">
            @{ var disabled = isedittable;
                var markreadonly = "mark-readonly";}
            @if (!Model.ConfirmedEmail && Model.Employee.IsUser)
            {<div class="row">
                    <label class="text text-danger-centarix">Must confirm email before resetting password</label>
                </div>
                disabled = "disabled";
                markreadonly = "";
            }
            <div class="row">

                <div class="col-6  p-0">
                    <label asp-for="Password" class="control-label"></label> &nbsp;&nbsp;&nbsp;&nbsp;

                    <div class="input-group">
                        <input asp-for="Password" class="form-control-plaintext border-bottom " autocomplete="off" readonly />
                        <span class="input-group-text">
                            <i type="button" class="icon-remove_red_eye-24px float-right " style="color: var(--users-color); padding-top: .55rem; font-size: 1.25rem;" id="ViewPassword"></i>
                            @*<input type="button" class="custom-button custom-button-small-font users-background-color px-1 py-0" id="ViewPassword" value="View" />*@
                        </span>
                    </div>
                    @*<input asp-for="Password" class="form-control-plaintext border-bottom" autocomplete="off" />*@
                    <span asp-validation-for="Password" class="text-danger-centarix"></span>
                </div>
                <div class="col-6">
                    @*<label asp-for="ConfirmPassword" class="control-label"></label>
                                <input asp-for="ConfirmPassword" class="form-control-plaintext border-bottom" autocomplete="off" readonly />
                                <span asp-validation-for="ConfirmPassword" class="text-danger-centarix"></span>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-6 p-0">*@
                    <label asp-for="Employee.SecureAppPass" class="control-label"></label>
                    <input asp-for="Employee.SecureAppPass" value="" class="form-control-plaintext border-bottom mark-readonly @isedittable @modaltype" autocomplete="off" />
                    <span asp-validation-for="Employee.SecureAppPass" class="text-danger-centarix"></span>
                </div>

            </div>
            <div class="row">
                <div class="col-6">
                    <input type="button" id="GeneratePassword" class="black-text py-2 mr-6 btn text rounded-pill yearly-monthly  text-center  px-5  border border-dark black-text @markreadonly" value="Generate Password" @disabled />
                </div>
            </div>
        </div>
        <div id="permissions" class="tab-pane fade @showPermissions">
