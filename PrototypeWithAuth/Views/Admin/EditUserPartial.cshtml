                    @*<select class="mdb-select custom select-dropdown form-control-plaintext only-employee mark-readonly job-category" asp-for="NewEmployee.JobSubcategoryTypeID" searchable="default value" disabled>
                        @foreach (var category in Model.JobSubcategoryTypes)
                        {
                            <option value="@category.JobSubcategoryTypeID">@category.Description</option>
                        }
                    </select>*@
                    <span asp-validation-for="NewEmployee.JobSubcategoryTypeID" class="text-danger-centarix"></span>
                </div>
                <div class="col-4 remove-padding-user @(hidden=="hidden"?"p-0":"")">
