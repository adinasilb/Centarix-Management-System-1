                        double hoursWorked = Model.NewEmployee.SalariedEmployee?.HoursPerDay ?? 0;
                    }
                    <input class="form-control-plaintext border-bottom mark-readonly" id="TimeSpan-HoursPerDay" type="time" value="@TimeSpan.FromHours(hoursWorked)" disabled />
                    <span asp-validation-for="NewEmployee.SalariedEmployee.HoursPerDay" class="text-danger-centarix"></span>
                </div>
                <div class="col-4">
                    <label class="control-label" asp-for="NewEmployee.VacationDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly" asp-for="NewEmployee.VacationDays" disabled />
                    <span class="text-danger-centarix" asp-validation-for="NewEmployee.VacationDays"></span>
                </div>
            </div>
            <div class="row">
                <div class="col-4">
                    <label class="control-label" asp-for="NewEmployee.RollOverVacationDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly" asp-for="NewEmployee.RollOverVacationDays" />
                    <span class="text-danger-centarix" asp-validation-for="NewEmployee.RollOverVacationDays"></span>
                </div>
                <div class="col-4">
                    <label class="control-label" asp-for="NewEmployee.RollOverSickDays"></label>
                    <input class="form-control-plaintext border-bottom mark-readonly" asp-for="NewEmployee.RollOverSickDays"  />
                    <span class="text-danger-centarix" asp-validation-for="NewEmployee.RollOverSickDays"></span>
                </div>
            </div>
        </div>

            <div id="security" class="tab-pane fade @showSecurity">
                @{ var disabled = "";
                    var markreadonly = "mark-readonly";}
                @if (!Model.ConfirmedEmail && Model.NewEmployee.IsUser)
                {<div class="row">
                        <label class="text text-danger-centarix">Must confirm email before resetting password</label>
                    </div>
                    disabled = "disabled";
                    markreadonly = "";
                }
                <div class="row">

                    <div class="col-6  p-0">
                        <label asp-for="Password" class="control-label"></label> &nbsp;&nbsp;&nbsp;&nbsp;

                        <div class="input-group">
                            <input asp-for="Password" class="form-control-plaintext border-bottom " autocomplete="off" readonly />
                            <span class="input-group-text">
                                <i type="button" class="icon-remove_red_eye-24px float-right " style="color: var(--users-color); padding-top: .55rem; font-size: 1.25rem;" id="ViewPassword"></i>
                                @*<input type="button" class="custom-button custom-button-small-font users-background-color px-1 py-0" id="ViewPassword" value="View" />*@
                            </span>
                        </div>
                        @*<input asp-for="Password" class="form-control-plaintext border-bottom" autocomplete="off" />*@
                        <span asp-validation-for="Password" class="text-danger-centarix"></span>
                    </div>
                    <div class="col-6">
                        @*<label asp-for="ConfirmPassword" class="control-label"></label>
                            <input asp-for="ConfirmPassword" class="form-control-plaintext border-bottom" autocomplete="off" readonly />
                            <span asp-validation-for="ConfirmPassword" class="text-danger-centarix"></span>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-6 p-0">*@
                        <label asp-for="SecureAppPass" class="control-label"></label>
