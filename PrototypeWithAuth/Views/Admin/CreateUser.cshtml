@model PrototypeWithAuth.ViewModels.RegisterUserViewModel;
@using PrototypeWithAuth.AppData;

<script src="~/js/CreateUser.js"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.min.js")"
        type="text/javascript">
</script>

<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")"
        type="text/javascript">
</script>

<script src="~/js/site.js" asp-append-version="true"></script>
@{
    ViewData["Title"] = "CreateUser";
    Layout = "~/Views/Shared/RequestNavView.cshtml";
}
<div class="card col-8 offset-2 create-user users-form" style="width:100%; box-shadow: 0px 2px 6px #5656567A;">
    <div class="card-body">
        <form action="" id="createModalForm" method="post" enctype="multipart/form-data" style="height: 100%; overflow: auto;">
            @* HIDDEN INPUTS TO DISABLE AUTOFILLS *@
            <input type="email" class="hidden" />
            <input type="password" class="hidden" />

            <div class="new-modal-header border-bottom container-fluid">
                <div class="row modal-sides">
                    <div class="col-8 offset-0">
                        <h2 class="mt-1">New User</h2>
                    </div>
                    <div class="col-4">
                        <input type="submit" id="addUser" value="Add User" asp-action="CreateUser" class="custom-button custom-button-font btn-block disabled-submit" style="float: right;" />
                    </div>
                </div>
                <div class="row modal-sides div-tabs">
                    <div class="col-12 px-0">
                        <div class="container-fluid p-0 text-center">
                            <ul class="nav nav-tabs container-fluid p-0 border-bottom-0" style="display:inline-flex; justify-content:space-between; color:#666666;">
                                <li class="nav-item">
                                    <a data-toggle="tab" href="#details" id="user-details-tab" class="nav-link user-nav-link">
                                        <i class="icon-face-24px" style="font-size: 2rem;"></i><br />Details
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a data-toggle="tab" href="#permissions" id="user-permissions-tab" class="nav-link user-nav-link permissions-tab">
                                        <i class="icon-https-24px" style="font-size: 2rem;"></i><br />Permissions
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a data-toggle="tab" href="#budget" id="user-budget-tab" class="nav-link user-nav-link">
                                        <i class="icon-monetization_on-24px" style="font-size: 2rem;"></i><br />Budget
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a data-toggle="tab" href="#images" id="user-images-tab" class="nav-link user-nav-link">
                                        <i class="icon-account_box-24px-1" style="font-size: 2rem;"></i><br />Image
                                    </a>
                                </li>
                                <li class="nav-item">
                                    <a data-toggle="tab" href="#more" id="user-more-tab" class="nav-link user-nav-link">
                                        <i class="icon-more-24px" style="font-size: 2rem;"></i><br />More
                                    </a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
            <div class="new-modal-body">
                <div class="div-tabs modal-sides" style="background: #FFF; " @*border: 2px solid #EAEAEB;"*@>
                    <div class="tab-content">
                        <div id="details" class="tab-pane fade">
                            <div class="row ">
                                <input type="hidden" asp-for="NewEmployee.EmployeeStatusID" />
                                @{ var first = true; }
                                @foreach (var status in Model.EmployeeStatuses)
                                {
                                    var id = "EmployeeStatusID" + status.EmployeeStatusID;
                                    if (first)
                                    {
                                        <div class="custom-control custom-radio mr-4 ml-4 employee-type">
                                            @Html.RadioButtonFor(model => model.NewEmployee.EmployeeStatusID, status.EmployeeStatusID, htmlAttributes: new { @class = "custom-control-input", @id = id, @checked = "true" })
                                            <label class="radio-button custom-control-label pt-1 pl-2" for="@id">@status.Description</label>
                                        </div>
                                    }
                                    else
                                    {
                                        <div class="custom-control custom-radio mr-4 ml-4 employee-type">
                                            @Html.RadioButtonFor(model => model.NewEmployee.EmployeeStatusID, status.EmployeeStatusID, htmlAttributes: new { @class = "custom-control-input", @id = id })
                                            <label class="radio-button custom-control-label pt-1 pl-2" for="@id">@status.Description</label>
                                        </div>
                                        first = false;
                                    }
                                }
                            </div>
                            <div class="row">
                                <div class="col-6">
                                    <label asp-for="FirstName" class="control-label"></label>
                                    <input asp-for="FirstName" class="form-control-plaintext border-bottom" />
                                    <span asp-validation-for="FirstName" class="text-danger-centarix"></span>
                                </div>
                                <div class="col-6">
                                    <label asp-for="LastName" class="control-label"></label>
                                    <input asp-for="LastName" class="form-control-plaintext border-bottom" />
                                    <span asp-validation-for="LastName" class="text-danger-centarix"></span>
                                </div>
                            </div>

                            <div class="row">
                                <div class="col-6">
                                    <label asp-for="CentarixID" class="control-label"></label>
                                    <input asp-for="CentarixID" class="form-control-plaintext border-bottom" />
                                    <span asp-validation-for="CentarixID" class="text-danger-centarix"></span>
                                </div>
                                <div class="col-6 ">
                                    <label asp-for="NewEmployee.JobTitle" class="control-label"></label>
                                    <input asp-for="NewEmployee.JobTitle" class="form-control-plaintext border-bottom" required />
                                    <span asp-validation-for="NewEmployee.JobTitle" class="text-danger-centarix"></span>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-4">
                                    <label class="control-label">Job Category</label>
                                    @Html.DropDownListFor(
                                        vm => vm.NewEmployee.JobCategoryTypeID,
                                        new SelectList(
                                        Model.JobCategoryTypes,
                                        dataValueField: "JobCategoryTypeID",
                                        dataTextField: "Description"),
                                        optionLabel: "Select A Category",
                                        htmlAttributes: new { @class = "mdb-select custom select-dropdown form-control-plaintext", @required = "true" }
                                        )
                                    <span asp-validation-for="NewEmployee.JobCategoryTypeID" class="text-danger-centarix"></span>
                                </div>
                                <div class="col-4">
