
@using Microsoft.AspNetCore.Identity
@using PrototypeWithAuth.Data
@using PrototypeWithAuth.AppData
@using PrototypeWithAuth.ViewModels
@model SummaryHoursViewModel; 

@inject UserManager<ApplicationUser> UserManager

@{
    ViewData["Title"] = "Summary Hours";
    if (AppUtility.IsAjaxRequest(Context.Request))
    {
        Layout = null;
        <script src="~/js/site.js" asp-append-version="true"></script>
    }
    else
    {
        Layout = "~/Views/Shared/RequestNavView.cshtml";
    }


}
<div class="row error-message">
    @{ await Html.RenderPartialAsync("_ErrorMessage", Model.ErrorMessage); }
</div>
<div class=" container-fluid" id="hoursTable">
    @{await Html.RenderPartialAsync("HoursPage", Model); }  
</div>
